[
    [
        1,
        " وَٱلسَّمَآءِ ذَاتِ ٱلْبُرُوجِ",
        "Al-Buruj",
        590
    ],
    [
        2,
        "وَٱلْيَوْمِ ٱلْمَوْعُودِ",
        "Al-Buruj",
        590
    ],
    [
        3,
        "وَشَاهِدٍ وَمَشْهُودٍ",
        "Al-Buruj",
        590
    ],
    [
        4,
        "قُتِلَ أَصْحَـٰبُ ٱلْأُخْدُودِ",
        "Al-Buruj",
        590
    ],
    [
        5,
        "ٱلنَّارِ ذَاتِ ٱلْوَقُودِ",
        "Al-Buruj",
        590
    ],
    [
        6,
        "إِذْ هُمْ عَلَيْهَا قُعُودٌ",
        "Al-Buruj",
        590
    ],
    [
        7,
        "وَهُمْ عَلَىٰ مَا يَفْعَلُونَ بِٱلْمُؤْمِنِينَ شُهُودٌ",
        "Al-Buruj",
        590
    ],
    [
        8,
        "وَمَا نَقَمُوا۟ مِنْهُمْ إِلَّآ أَن يُؤْمِنُوا۟ بِٱللَّهِ ٱلْعَزِيزِ ٱلْحَمِيدِ",
        "Al-Buruj",
        590
    ],
    [
        9,
        "ٱلَّذِى لَهُۥ مُلْكُ ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضِ ۚ وَٱللَّهُ عَلَىٰ كُلِّ شَىْءٍ شَهِيدٌ",
        "Al-Buruj",
        590
    ],
    [
        10,
        "إِنَّ ٱلَّذِينَ فَتَنُوا۟ ٱلْمُؤْمِنِينَ وَٱلْمُؤْمِنَـٰتِ ثُمَّ لَمْ يَتُوبُوا۟ فَلَهُمْ عَذَابُ جَهَنَّمَ وَلَهُمْ عَذَابُ ٱلْحَرِيقِ",
        "Al-Buruj",
        590
    ],
    [
        11,
        "إِنَّ ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّـٰلِحَـٰتِ لَهُمْ جَنَّـٰتٌ تَجْرِى مِن تَحْتِهَا ٱلْأَنْهَـٰرُ ۚ ذَٰلِكَ ٱلْفَوْزُ ٱلْكَبِيرُ",
        "Al-Buruj",
        590
    ],
    [
        12,
        "إِنَّ بَطْشَ رَبِّكَ لَشَدِيدٌ",
        "Al-Buruj",
        590
    ],
    [
        13,
        "إِنَّهُۥ هُوَ يُبْدِئُ وَيُعِيدُ",
        "Al-Buruj",
        590
    ],
    [
        14,
        "وَهُوَ ٱلْغَفُورُ ٱلْوَدُودُ",
        "Al-Buruj",
        590
    ],
    [
        15,
        "ذُو ٱلْعَرْشِ ٱلْمَجِيدُ",
        "Al-Buruj",
        590
    ],
    [
        16,
        "فَعَّالٌ لِّمَا يُرِيدُ",
        "Al-Buruj",
        590
    ],
    [
        17,
        "هَلْ أَتَىٰكَ حَدِيثُ ٱلْجُنُودِ",
        "Al-Buruj",
        590
    ],
    [
        18,
        "فِرْعَوْنَ وَثَمُودَ",
        "Al-Buruj",
        590
    ],
    [
        19,
        "بَلِ ٱلَّذِينَ كَفَرُوا۟ فِى تَكْذِيبٍ",
        "Al-Buruj",
        590
    ],
    [
        20,
        "وَٱللَّهُ مِن وَرَآئِهِم مُّحِيطٌۢ",
        "Al-Buruj",
        590
    ],
    [
        21,
        "بَلْ هُوَ قُرْءَانٌ مَّجِيدٌ",
        "Al-Buruj",
        590
    ],
    [
        22,
        "فِى لَوْحٍ مَّحْفُوظٍۭ",
        "Al-Buruj",
        590
    ]
]