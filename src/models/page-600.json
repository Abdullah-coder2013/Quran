[
    [
        1,
        "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَٱلْعَصْرِ"
    ],
    [
        2,
        "إِنَّ ٱلْإِنسَٰنَ لَفِى خُسْرٍ"
    ],
    [
        3,
        "إِلَّا ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّٰلِحَٰتِ وَتَوَاصَوْا۟ بِٱلْحَقِّ وَتَوَاصَوْا۟ بِٱلصَّبْرِ"
    ],
    [
        1,
        "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَيْلٌۭ لِّكُلِّ هُمَزَةٍۢ لُّمَزَةٍ"
    ],
    [
        2,
        "ٱلَّذِى جَمَعَ مَالًۭا وَعَدَّدَهُۥ"
    ],
    [
        3,
        "يَحْسَبُ أَنَّ مَالَهُۥٓ أَخْلَدَهُۥ"
    ],
    [
        4,
        "كَلَّا ۖ لَيُنۢبَذَنَّ فِى ٱلْحُطَمَةِ"
    ],
    [
        5,
        "وَمَآ أَدْرَىٰكَ مَا ٱلْحُطَمَةُ"
    ],
    [
        6,
        "نَارُ ٱللَّهِ ٱلْمُوقَدَةُ"
    ],
    [
        7,
        "ٱلَّتِى تَطَّلِعُ عَلَى ٱلْأَفْـِٔدَةِ"
    ],
    [
        8,
        "إِنَّهَا عَلَيْهِم مُّؤْصَدَةٌۭ"
    ],
    [
        9,
        "فِى عَمَدٍۢ مُّمَدَّدَةٍۭ"
    ],
    [
        1,
        "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِأَصْحَٰبِ ٱلْفِيلِ"
    ],
    [
        2,
        "أَلَمْ يَجْعَلْ كَيْدَهُمْ فِى تَضْلِيلٍۢ"
    ],
    [
        3,
        "وَأَرْسَلَ عَلَيْهِمْ طَيْرًا أَبَابِيلَ"
    ],
    [
        4,
        "تَرْمِيهِم بِحِجَارَةٍۢ مِّن سِجِّيلٍۢ"
    ],
    [
        5,
        "فَجَعَلَهُمْ كَعَصْفٍۢ مَّأْكُولٍۭ"
    ]
]